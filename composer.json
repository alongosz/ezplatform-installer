{
    "name": "ezsystems/ezplatform-installer",
    "description": "eZ Platform v2 Installer",
    "homepage": "https://ezplatform.com/",
    "license": "GPL-2.0",
    "type": "ezplatform-bundle",
    "autoload": {
        "psr-4": {
            "EzSystems\\EzPlatformInstallerBundle\\": "src/bundle",
            "EzSystems\\EzPlatformInstaller\\": "src/lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EzSystems\\Tests\\EzPlatformInstallerBundle\\": "src/tests/bundle",
            "EzSystems\\Tests\\EzPlatformInstaller\\": "src/tests/lib"
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/symfony": "^3.4@rc",
        "ezsystems/ezpublish-kernel": "^7.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "friendsofphp/php-cs-fixer": "~2.7.1"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
